{
    "rulesDirectory": ["tslint-plugin-prettier"],
    "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-microsoft-contrib/recommended", "tslint-config-prettier"],
    "rules": {
        "import-name": false,
        "no-console": [true, "log", "error"],
        "no-consecutive-blank-lines": [true, 1],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "no-implicit-dependencies": false,
        "no-submodule-imports": false,
        "match-default-export-name": false,
        "no-default-export": false,
        "newline-before-return": false,
        "export-name": false,
        "no-relative-imports": false,
        "only-arrow-functions": true,
        "function-name": false,
        "typedef": [true, "property-declaration", "variable-declaration"],
        "callable-types": false,
        "variable-name": [true, "check-format", "allow-pascal-case"],
        "mocha-avoid-only": false,
        "mocha-no-side-effect-code": false,
        "mocha-unneeded-done": false,
        "no-constant-condition": false,
        "await-promise": [true, "Bluebird"],
        "no-any": false,
        "prettier": [true, {
            "singleQuote": true
        }]
    }
}