{
  "name": "nestattacked-civilization-kit",
  "version": "0.0.1",
  "main": "./dist/lib/index.js",
  "license": "MIT",
  "bin": {
    "ncc": "./dist/bin/ncc.js"
  },
  "scripts": {
    "after-build": "chmod a+x dist/bin/ncc.js && rm dist/lib/api -r -f && mkdir dist/lib/api && cp src/lib/api/index.* dist/lib/api",
    "dev": "tsc-watch --onSuccess \"yarn after-build\"",
    "build": "rm dist -r -f && tsc -p tsconfig.build.json && yarn after-build"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.1.0",
    "load-json-file": "^5.2.0",
    "lodash": "^4.17.11",
    "typescript": "^3.3.3333",
    "typescript-to-lua": "^0.15.2",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.10.4",
    "@types/sql-bricks": "^2.0.1",
    "@types/xml": "^1.0.3",
    "ts-node": "^8.0.3",
    "tsc-watch": "^2.1.2"
  }
}
